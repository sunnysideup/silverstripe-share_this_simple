{
    "name": "sunnysideup/share_this_simple",
    "type": "silverstripe-vendormodule",
    "description": "Silverstripe Module that provides a simple way to add share this links to your website.",
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "share",
        "social media",
        "share links",
        "facebook",
        "pinterest",
        "twitter",
        "Site Tree Extensions"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "require": {
        "silverstripe/recipe-cms": "~4@stable"
    },
    "extra": {
        "expose": [
            "client/javascript"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\ShareThisSimple\\": "src/"
        }
    }
}
